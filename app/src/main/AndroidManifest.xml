<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sehalsein.e_park">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity
            android:name=".UserActivites.UserHomeActivity"
            android:label="@string/title_activity_user_home" />
        <activity
            android:name=".AdminActivity.AdminHomeActivity"
            android:label="@string/title_activity_admin_home" />
        <activity android:name=".AdminActivity.AddNewParkingArea" />
        <activity android:name=".AdminActivity.AdminEditParkingArea" />
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".UserActivites.BookParkingActivity" />
        <activity android:name=".UserActivites.BookingValidation" />
        <activity android:name=".Activity.PaymentActivity"></activity>
    </application>

</manifest>